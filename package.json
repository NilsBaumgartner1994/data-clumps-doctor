{
  "name": "data-clumps-doctor",
  "version": "0.1.101",
  "description": "This is a package to identify data-clumps",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "coverage": "jest --coverage",
    "test": "yarn run build && yarn run testOnly",
    "testOnly": "jest --coverage=false",
    "generate-missing-test-reports": "yarn run build && node ./build/tests/generateMissingReports.js",
    "regenerate-existing-test-report-expected": "yarn run build && node ./build/tests/regenerateExistingReports.js",
    "start": "yarn run build && node ./build/ignoreCoverage/development.js",
    "build": "rimraf ./build && tsc && mkdir -p ./build/src/ignoreCoverage && cp -r ./src/ignoreCoverage/astGenerator ./build/src/ignoreCoverage/ && cp -r ./build/src/ignoreCoverage ./build/ignoreCoverage && cp ./build/src/index.js ./build/index.js && cp ./build/src/index.d.ts ./build/index.d.ts && cp package.json ./build && cp ./README.md ./build && cp ./LICENSE.md ./build && npm run chmodCli",
    "chmodCli": "chmod +x ./build/ignoreCoverage/cli.js",
    "dev": "nodemon",
    "_prepareUpload": "yarn run build && rimraf ./coverage && rimraf ./build/tests",
    "_uploadBuild": "cd ./build && yarn pack && yarn publish && cd ..",
    "_uploadWithoutPatch": "yarn run _prepareUpload && yarn run _uploadBuild && yarn run clean",
    "upload": "yarn version patch && yarn run _uploadWithoutPatch",
    "clean": "rimraf ./build && gts clean",
    "format": "prettier . --write",
    "lint": "eslint . --ext .js,.ts,.tsx --fix",
    "setup": "yarn install && node setup.js && rimraf setup.js setup.json"
  },
  "bin": {
    "data-clumps": "./build/ignoreCoverage/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NilsBaumgartner1994/data-clumps"
  },
  "keywords": [
    "data-clumps",
    "code-smell",
    "code-smells",
    "code-smell-detection",
    "detector"
  ],
  "author": "Nils Baumgartner",
  "contributors": [
    {
      "name": "Nils Baumgartner",
      "email": "nilsbaumgartner1994@gmail.com",
      "url": "https://github.com/NilsBaumgartner1994"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NilsBaumgartner1994/data-clumps/issues"
  },
  "homepage": "https://github.com/NilsBaumgartner1994/data-clumps",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/levenshtein": "^1.0.4",
    "@types/node": "^16.0.0",
    "gts": "^3.1.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.9",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@azure/dtdl-parser": "^1.0.0-beta.2",
    "@babel/preset-typescript": "^7.21.0",
    "@digicatapult/dtdl-parser": "^1.0.36",
    "@stdlib/buffer-from-string": "^0.0.8",
    "Base64": "^1.1.0",
    "axios": "^0.21.4",
    "base64-js": "^1.5.1",
    "buffer": "^6.0.0",
    "buffer-from": "^1.1.2",
    "commander": "^10.0.1",
    "data-clumps-type-context": "^0.1.102",
    "eslint": "^7.32.0",
    "levenshtein": "^1.0.5",
    "minimist": "^1.2.8",
    "p-limit": "^3.1.0",
    "simple-git": "^3.19.0",
    "stream-chain": "^3.4.0",
    "stream-json": "^1.9.1",
    "ts-morph": "^17.0.1",
    "util": "^0.12.5",
    "xml2js": "^0.6.2"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
